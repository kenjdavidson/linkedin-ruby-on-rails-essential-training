<% content_for(:head_title) { "Add New Subject" }%>

<p>
<%= link_to('<< Back to Subjects', subjects_path) %>
</p>

<h2>Add Subject</h2>

<%= form_for(@subject, :url => subjects_path, :method => 'post') do |f| %>

<%= render(partial: 'form', locals: { f: f }) %>

<div class="form-buttons">
  <%= f.submit("Create")%>
</div>
<% end %>